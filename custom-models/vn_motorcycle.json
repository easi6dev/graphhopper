{
    "priority": [
        {
            "if": "road_class == MOTORWAY || road_class == TRUNK",
            "multiply_by": "0.2"
        },
        {
            "if": "track_type.ordinal() > 1",
            "multiply_by": "0"
        },
        {
            "if": "car_access == false || road_environment == FERRY || road_access == NO || road_access == PRIVATE || road_access == CUSTOMERS",
            "multiply_by": "0"
        }
    ],
    "distance_influence": 10,
    "heading_penalty": 180
}
